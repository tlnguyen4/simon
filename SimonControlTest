    %store/vec4 v0x7febe3528040_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7febe3528040_0, 0, 1;
    %delay 10, 0;
    %load/vec4 v0x7febe3528300_0;
    %cmpi/ne 1, 0, 3;
    %jmp/0xz  T_5.36, 6;
    %vpi_call 2 125 "$display", "\011[FAILURE]:%s", "Finished guessing sequence and should return to INPUT." {0 0 0};
T_5.36 ;
    %delay 0, 0;
    %load/vec4 v0x7febe3528180_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_5.38, 6;
    %vpi_call 2 126 "$display", "\011[FAILURE]:%s", "n should not be incremented when just returned to INPUT state." {0 0 0};
T_5.38 ;
    %delay 0, 0;
    %load/vec4 v0x7febe3527fb0_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_5.40, 6;
    %vpi_call 2 127 "$display", "\011[FAILURE]:%s", "i should not be cleared" {0 0 0};
T_5.40 ;
    %delay 0, 0;
    %load/vec4 v0x7febe3528230_0;
    %pad/u 32;
    %cmpi/ne 1, 0, 32;
    %jmp/0xz  T_5.42, 6;
    %vpi_call 2 128 "$display", "\011[FAILURE]:%s", "pattern led should show user input." {0 0 0};
T_5.42 ;
    %delay 0, 0;
    %vpi_call 2 127 "$finish" {0 0 0};
    %end;
    .thread T_5;
